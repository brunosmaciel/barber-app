{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "prettier",
      "next/core-web-vitals"
  ],
  "overrides": [
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "prettier"
  ],
  "rules": {
    "react/react-in-jsx-scope":0,
    "prettier/prettier": "error"
  }
}